{
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "importPath": "relative",
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  },
  "name": "sample-typescript-app",
  "bsc-flags": ["-bs-super-errors"],
  "ppx-flags": [
    "../../_esy/default/build/install/default/bin/deadcodeppx.exe",
    "../../_esy/default/build/install/default/bin/lazyLoad.exe"
  ],
  "reason": { "react-jsx": 3 },
  "bs-dependencies": ["reason-react", "@glennsl/bs-json"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "refmt": 3
}
