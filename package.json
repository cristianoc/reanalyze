{
  "name": "reanalyze",
  "version": "1.13.0",
  "private": true,
  "description": "Analyzers for Dead Code/Types and termination",
  "license": "MIT",
  "scripts": {
    "build": "npx esy",
    "clean": "npx esy clean",
    "test": "node ./scripts/run_integration_tests.js",
    "install:examples": "(cd examples/deadcode && npm install) & (cd examples/termination && npm install)",
    "build:examples": "(cd examples/deadcode && npm run clean && npm run build && npm run analyze) & (cd examples/termination && npm run clean && npm run build && npm run analyze)",
    "dce": "./_esy/default/build/install/default/bin/reanalyze.exe -suppress src/nothing -unsuppress doesnotexist -dce-cmt `find  _esy/default/store/b/reanalyze-*/default/src/.Reanalyze.eobjs -name byte`",
    "exception": "./_esy/default/build/install/default/bin/reanalyze.exe -exception-cmt `find  _esy/default/store/b/reanalyze-*/default/src/.Reanalyze.eobjs -name byte`",
    "exception409": "./_esy/409/build/install/default/bin/reanalyze.exe -exception-cmt `find  _esy/409/store/b/reanalyze-*/default/src/.Reanalyze.eobjs -name byte`",
    "preversion": "npm test",
    "version": "node scripts/bump_version_module.js && git add -A src/",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "debug": "^4.1.0",
    "esy": "0.5.8"
  },
  "author": "Cristiano Calcagno",
  "bugs": {
    "url": "https://github.com/cristianoc/reanalyze/issues"
  },
  "homepage": "https://github.com/cristianoc/reanalyze#readme"
}
